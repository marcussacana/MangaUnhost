  <data name="ComicTlReaderHtmlBase" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;
&lt;html lang="en"&gt;
&lt;head&gt;
  &lt;meta charset="UTF-8"&gt;
  &lt;meta name="viewport" content="width=device-width, initial-scale=1.0"&gt;
  &lt;style&gt;
	body {{
	  margin: 0;
	  background-color: #000000;
	}}

	.reader {{
	  position: absolute;
	  margin: auto;
	  top: 0;
	  right: 0;
	  bottom: 0;
	  left: 0;
	  width: max-content;
	  background-color: #ccc;
	}}
	.container {{
	  position: relative;
	  width: max-content;
	  height: max-content;
	  margin-bottom: -5px;
	  overflow: hidden;
	}}
	
	.overlay {{
	  position: absolute;
	  top: 0;
	  left: 0;
	  background-size: cover;
	}}
  &lt;/style&gt;
  &lt;title&gt;{0}&lt;/title&gt;
&lt;/head&gt;
&lt;body&gt;
	&lt;div class="reader"&gt;
	  {1}
	&lt;/div&gt;
  &lt;script&gt;
	document.addEventListener('DOMContentLoaded', function () {{
		let circleSize = getCircleSize();
		function getCircleSize() {{
			var size = location.hash;
			if (size.indexOf("#") &gt;= 0) {{
			  size = size.split('#')[1];
			  return parseInt(size);
			}}
			return 100;
		}}
		function updateCircleSize(size) {{
			location.hash = '#' + size;
			next.href = next.href.split('#')[0] + location.hash;
		}}
		const containers = Array.from(document.getElementsByClassName("container")).forEach((container, index) =&gt; {{
			const imageContainer = container;
			const imageOverlay = imageContainer.getElementsByClassName('overlay')[0];

			function updateClipPath(mouseX, mouseY) {{
			  imageOverlay.style.clipPath = `circle(${{circleSize}}px at ${{mouseX}}% ${{mouseY}}%)`;
			}}

			function getMousePosition(e) {{
			  const rect = imageContainer.getBoundingClientRect();
			  const mouseX = (e.clientX - rect.left) / rect.width * 100;
			  const mouseY = (e.clientY - rect.top) / rect.height * 100;
			  return {{ mouseX, mouseY }};
			}}

			imageContainer.addEventListener('mousemove', function (e) {{
			  const {{ mouseX, mouseY }} = getMousePosition(e);
			  updateClipPath(mouseX, mouseY);
			}});

			imageContainer.addEventListener('mouseleave', function () {{
			  imageOverlay.style.clipPath = 'circle(0 at 0 0)';
			}});

			let isCtrlPressed = false;
			let isShiftPressed = false;

			window.addEventListener('keydown', function (e) {{
			  if (e.key === 'Control') {{
				isCtrlPressed = true;
			  }}
			  if (e.key === 'Shift') {{
				isShiftPressed = true;
			  }}
			}});

			window.addEventListener('keyup', function (e) {{
			  if (e.key === 'Control') {{
				isCtrlPressed = false;
			  }}
			  if (e.key === 'Shift') {{
				isShiftPressed = false;
			  }}
			}});

			imageContainer.addEventListener('wheel', function (e) {{
			  if (isCtrlPressed &amp;&amp; isShiftPressed) {{
				const {{ mouseX, mouseY }} = getMousePosition(e);

				if (isInsideImage(mouseX, mouseY)) {{
				  circleSize += e.deltaY &gt; 0 ? -10 : 10;
				  circleSize = Math.max(0, circleSize);
				  updateCircleSize(circleSize);

				  updateClipPath(mouseX, mouseY);
				  e.preventDefault();
				}}
			  }}
			}});

			function isInsideImage(mouseX, mouseY) {{
			  return mouseX &gt;= 0 &amp;&amp; mouseX &lt;= 100 &amp;&amp; mouseY &gt;= 0 &amp;&amp; mouseY &lt;= 100;
			}}
		}});
	}});
  &lt;/script&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
